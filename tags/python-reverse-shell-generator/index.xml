&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="nb">print&lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="s2">&amp;#34;[!] INTERFACES:&amp;#34;&lt;/span>&lt;span class="p">)&lt;/span>
